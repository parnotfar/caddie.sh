<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.caddie.voice</string>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/local/bin/caddie-voice</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>KeepAlive</key>
	<true/>
	<key>StandardOutPath</key>
	<string>/tmp/caddie-voice.log</string>
	<key>StandardErrorPath</key>
	<string>/tmp/caddie-voice-error.log</string>
</dict>
</plist>
